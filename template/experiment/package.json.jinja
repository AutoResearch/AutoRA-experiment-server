{
  "name": "experiment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "eslint --fix ./**/*.js && vite build",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.6.1",
    "eslint": "^8.54.0",
    "vite": "^5.2.13",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    {% if project_type == 'Reaction Time'%}"@jspsych/plugin-serial-reaction-time": "*",{% endif %}
    {% if project_type in ['HTML Button', 'Save trial parameters', 'Lexical decision']%}"@jspsych/plugin-html-button-response": "*",{% endif %}
    {% if project_type == 'Multi Choice Survey'%}"@jspsych/plugin-survey-multi-choice": "*",{% endif %}
    {% if project_type == 'Multi Select Survey'%}"@jspsych/plugin-survey-multi-select": "*",{% endif %}
    {% if project_type in ['Save trial parameters', 'Pause/Unpause', 'Lexical decision', 'Basic', 'JsPsych - RDK', 'JsPsych - Stroop', 'SweetBean'] %}"@jspsych/plugin-html-keyboard-response": "*",{% endif %}
    {% if project_type == 'Save trial parameters'%}"@jspsych/plugin-canvas-keyboard-response": "*",{% endif %}
    {% if project_type == 'Canvas Slider Response'%}"@jspsych/plugin-canvas-slider-response": "*",{% endif %}
    {% if project_type in ['Basic', 'JsPsych - RDK']%}"@jspsych-contrib/plugin-rdk": "*",{% endif %}
    {% if project_type in ['Blank']%}"@jspsych/plugin-browser-check": "*",{% endif %}
    {% if project_type == 'SuperExperiment' %}"super-experiment": "*",{% endif %}
    "axios": "^1.7.4",
    "jspsych": "^7.3.4"
  }
}